        <h3 class="agile-title"></h3>

        <div>
            <fieldset>
                <form>
                    <table>
                        <th>Name</th>
                        <th>Adress</th>
                        <th>MailAdress</th>
                        <th>Tel</th>
                        <th>Type</th>
                        <th>Stars Number</th>
                        <th>Price Single</th>
                        <th>Price Double</th>
                        <th>Price Child</th>
                        <th>Image</th>
                        <th>Rating</th>


                        {% for hebergement in Hebergements %}
                            <tr>
                                <td>{{ hebergement.nom }}</td>
                                <td>{{ hebergement.adresse }}</td>
                                <td>{{ hebergement.adresseMail }}</td>
                                <td>{{ hebergement.numerotel }}</td>
                                <td>{{ hebergement.type }}</td>
                                <td>{{ hebergement.nbetoile }}</td>
                                <td>{{ hebergement.prixSingle }}</td>
                                <td>{{ hebergement.prixdouble }}</td>
                                <td>{{ hebergement.prixenfant }}</td>
                                <td><img src="{{ asset ('images/'~hebergement.image) }}" style=" height:10% " /></td>
                                <td> {{ hebergement.rating|rating }}</td>
                                <td><a href="{{ path('deleteHebergement',{'id':hebergement.id}) }}">Reserver</a></td>

                            </tr>
                        {% endfor %}

